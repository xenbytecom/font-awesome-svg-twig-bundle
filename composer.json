{
    "name": "xenbyte/font-awesome-svg-twig-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle to use FontAwesome svg icons as inline svg.",
    "keywords": ["twig", "i"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Brauner",
            "email": "s.brauner@xenbyte.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.2 || ^7",
        "symfony/twig-bundle": "^6.2 || ^7",
        "ext-dom": "*"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Xenbyte\\FontAwesomeSvgTwigBundle\\": "src/"
        }
    }
}
